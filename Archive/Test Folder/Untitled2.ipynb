    "def fahr_to_cent(temp_f):\n",
    "    if type(temp_f) is not int:\n",
    "        return \"Not a number\"\n",
    "    elif temp_f < -459.67:\n",
    "        return \"Too cold!\"\n",
    "    else:\n",
    "        return ((temp_f - 32) * (5/9))"
